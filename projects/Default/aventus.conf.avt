{
	"module": "${{name}}",
	"componentPrefix": "${{componentPrefix}}",
	"build": [
		{
			"name": "Main",
			"src": [
				"./src/components/*",
				"./src/data/*",
				"./src/lib/*",
				"./src/ram/*",
				"./src/states/*"
			],
			"compile": [{
				"output": "./dist/${{name}}.js"
			}]
		}
	],
	"static": [
		{
			"name": "Main",
			"input": "./src/static/*",
			"output": "./dist/"
		}
	]
}