{
	"module": "${{name}}",
	"componentPrefix": "${{componentPrefix}}",
	"build": [
		{
			"name": "Main",
			"src": [
				"./src/components/*"
			],
			"compile": [{
				"output": "./dist/${{name}}.js"
			}]
		}
	],
	"static": [
		{
			"name": "Main",
			"input": "./static/",
			"output": "./dist/"
		}
	]
}