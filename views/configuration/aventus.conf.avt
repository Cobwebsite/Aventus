{
	"module": "dependances",
	"componentPrefix": "av",
	"build": [
		{
			"name": "Main",
			"hideWarnings": true,
			"src": [
				"./src/components/*",
				"./src/libs/*"
			],
			"compile": [
				{
					"output": "../../client/view/js/index.js"
				}
			]
		}
	],
	"static": [
		{
			"name": "Main",
			"input": "./src/static/",
			"output": "../../client/view/"
		}
	]
}